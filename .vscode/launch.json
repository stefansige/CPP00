{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Debug phonebook",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/ex01/phonebook", // Adjust for the correct path
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		],
		"miDebuggerPath": "/usr/bin/gdb", // Use "gdb" if it's in the system PATH
		"sourceFileMap": {
		  "/path/on/remote/machine": "${workspaceFolder}"
		}
	  }
	]
  }
  
  
  